<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backColor"
    android:orientation="vertical"
    tools:context="com.huagu.RX.rongxinmedical.Fragment.HomeFragment">

    <include layout="@layout/date_title"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_8"
                android:layout_marginLeft="@dimen/dimen_10"
                android:layout_marginRight="@dimen/dimen_10"
                android:layout_marginTop="@dimen/dimen_8"
                android:background="@drawable/home_item_bg"
                android:orientation="vertical"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >

                    <com.huagu.RX.rongxinmedical.View.CircleScheduleView
                        android:id="@+id/cirscview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        app:locat="0"
                        app:name="哈哈"
                        app:range="100"
                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_20"
                        android:layout_marginRight="@dimen/margin_10"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="@dimen/dimen_5"
                        >

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/name1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/circle_thinblue"
                            android:drawablePadding="@dimen/dimen_15"
                            android:gravity="center_vertical"
                            android:text="@string/Usage"
                            android:textSize="@dimen/textsize"/>

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/percent1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:minWidth="@dimen/dimen_20"
                            android:text="0/60"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:layout_marginRight="@dimen/margin_10"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="@dimen/dimen_5"
                        >

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/name2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/circle_thinorange"
                            android:drawablePadding="@dimen/dimen_15"
                            android:gravity="center_vertical"
                            android:text="@string/MaskFit"
                            android:textSize="@dimen/textsize"/>

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/percent2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:minWidth="@dimen/dimen_20"
                            android:text="0/15"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_20"
                        android:layout_marginRight="@dimen/margin_10"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="@dimen/dimen_5"
                        >

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/name3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/circle_thingray"
                            android:drawablePadding="@dimen/dimen_15"
                            android:gravity="center_vertical"
                            android:text="@string/Event"
                            android:textSize="@dimen/textsize"/>

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/percent3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:minWidth="@dimen/dimen_20"
                            android:text="0/20"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:layout_marginRight="@dimen/margin_10"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:padding="@dimen/dimen_5"
                        >

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/name4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:drawableLeft="@drawable/circle_thinyellow"
                            android:drawablePadding="@dimen/dimen_15"
                            android:gravity="center_vertical"
                            android:text="@string/Humidifier"
                            android:textSize="@dimen/textsize"/>

                        <com.huagu.RX.rongxinmedical.widget.CustomTextView
                            android:id="@+id/percent4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:minWidth="@dimen/dimen_20"
                            android:text="0/5"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!--<View-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="0.5dp"-->
            <!--android:layout_marginTop="@dimen/dimen_5"-->
            <!--android:background="@color/thin"/>-->

            <!--<View-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="10dp"-->
            <!--android:layout_marginBottom="@dimen/dimen_8"-->
            <!--android:background="@color/backColor"/>-->

            <com.huagu.RX.rongxinmedical.widget.MyListView
                android:id="@+id/mylistview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dimen_5"
                android:divider="@color/backColor"/>

        </LinearLayout>
    </ScrollView>
</LinearLayout>
